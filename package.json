{
  "name": "single-spa-angular",
  "version": "2.1.0",
  "description": "Helpers for building single-spa applications which use Angular 2",
  "main": "lib/single-spa-angular.js",
  "builders": "builders.json",
  "scripts": {
    "prepublish": "yarn build",
    "release": "standard-version",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "yarn clean && yarn lint && tsc",
    "clean": "rimraf lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CanopyTax/single-spa-angular.git"
  },
  "keywords": [
    "single-spa",
    "angular"
  ],
  "author": "Joel Denning",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/CanopyTax/single-spa-angular/issues"
  },
  "homepage": "https://github.com/CanopyTax/single-spa-angular#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }  
  },
  "peerDependencies": {
    "@angular-devkit/architect": ">= 0.10.0",
    "@angular-devkit/build-angular": ">= 0.10.0",
    "@angular-devkit/core": ">= 0.10.0",
    "rxjs": ">= 6.0.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.13.6",
    "@angular-devkit/build-angular": "^0.13.6",
    "@angular-devkit/core": "^7.3.6",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/node": "^11.11.3",
    "husky": "^1.3.1",
    "rimraf": "^2.6.2",
    "standard-version": "^5.0.1",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {}
}
